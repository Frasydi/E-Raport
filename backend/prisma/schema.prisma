generator client {
    provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
enum Role {
  Admin
  Operator
}

model Users {
    id  String @id @default(cuid())
    username String
    password String
    role Role @default(Admin)
    token String? @db.Text
}